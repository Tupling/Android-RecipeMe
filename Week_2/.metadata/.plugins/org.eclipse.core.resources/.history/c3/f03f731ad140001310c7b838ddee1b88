package com.daletupling.recipe;

public class Recipe {
	
	//Recipe's ENUM
	public enum Recipes{
		
		
		private final String recipeName;
		private final String recipeIngredients;
		
		//Set recipe Name and Ingredients to String variables
		private Recipes(String recipeName, String recipeIngredients){
			this.recipeName = recipeName;
			this.recipeIngredients = recipeIngredients;
		}
		
		//Set RecipeName
		public String setRecipe(){
			return recipeName;
		}
		//Set Recipe Ingredients
		public String setIngredients(){
			return recipeIngredients;
		}
		
	}

}
