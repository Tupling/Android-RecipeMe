package com.daletupling.recipe;

public enum Recipes {
	

	ChickenTendoori("Dinner", "Cayenne Pepper\r\nGarlic\r\n Lime Juice\r\n Salt\r\n Chicken Breast\r\nGround Coriander\r\nFresh Ginger\r\nPaprika\r\nFresh Ginger\r\nGround Cumin\r\nPlain Yogurt");
	
	private String recipeName;
	private String ingredients;
	
	private Recipes(String recipeName, String ingredients){

		this.recipeName = recipeName;
		this.ingredients = ingredients;
	}
	
		public static Object values() {
			// TODO Auto-generated method stub
			return null;
		}

	public String setRecipe() {
		// TODO Auto-generated method stub
		return recipeName;
	}



	public String setIngredients() {
		// TODO Auto-generated method stub
		return ingredients;
	}

}
